					</value>
				</li>
				<!--운반오프셋 추가 케리어-->
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="RB_Apron"]/equippedStatOffsets</xpath>
					<value>
						<MassOffset>+1</MassOffset>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="RB_Infantryvest"]/equippedStatOffsets</xpath>
