<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Rabbie The Moonrabbit race</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<!--운반오프셋 추가 가운-->
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="RB_Inteligown"]/equippedStatOffsets</xpath>
					<value>
						<MassOffset>+0.5</MassOffset>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="RB_Energygown"]/equippedStatOffsets</xpath>
